  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": "comment",
      "settings": { "foreground": "#A3A0B3B3", "fontStyle": "italic" }
    },
    { "scope": "constant", "settings": { "foreground": "#FFC887FF" } },
    {
      "scope": "constant.character.escape",
      "settings": { "foreground": "#7cffcdFF" }
    },
    { "scope": "entity.name", "settings": { "foreground": "#87CEFAFF" } },
    {
      "scope": "entity.name.function",
      "settings": { "foreground": "#7cffcdFF" }
    },
    {
      "scope": "entity.name.tag",
      "settings": { "foreground": "#FF99C0FF", "fontStyle": "bold" }
    },
    {
      "scope": ["entity.name.type", "storage.type.cs"],
      "settings": { "foreground": "#FFC887FF" }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#FFC887FF" }
    },
    {
      "scope": "entity.other.inherited-class",
      "settings": { "foreground": "#87CEFAFF" }
    },
    {
      "scope": "entity.other.attribute-name.id",
      "settings": { "foreground": "#7cffcdFF" }
    },
    {
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": { "foreground": "#87CEFAFF" }
    },
    {
      "scope": ["entity.name.variable", "variable"],
      "settings": { "foreground": "#FF99C0FF" }
    },
    {
      "scope": "keyword",
      "settings": { "foreground": "#C6A7FFFF", "fontStyle": "bold" }
    },
    { "scope": "keyword.operator", "settings": { "foreground": "#BBBBBB" } },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete"
      ],
      "settings": { "foreground": "#C6A7FFFF" }
    },
    {
      "scope": "keyword.other.unit",
      "settings": { "foreground": "#FFC887FF" }
    },
    {
      "scope": "markup.quote",
      "settings": { "foreground": "#87CEFAB3", "fontStyle": "italic" }
    },
    {
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#FF99C0FF", "fontStyle": "bold" }
    },
    {
      "scope": "markup.bold",
      "settings": { "foreground": "#C6A7FFFF", "fontStyle": "bold" }
    },
    {
      "scope": "markup.italic",
      "settings": { "foreground": "#7cffcdFF", "fontStyle": "italic" }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": { "foreground": "#FFC887FF" }
    },
    {
      "scope": "markup.underline.link",
      "settings": { "foreground": "#87CEFA" }
    },
    {
      "scope": "storage",
      "settings": { "foreground": "#C6A7FFFF", "fontStyle": "bold" }
    },
    {
      "scope": ["string.quoted", "string.template"],
      "settings": { "foreground": "#87CEFAFF" }
    },
    { "scope": "string.regexp", "settings": { "foreground": "#FFC887FF" } },
    { "scope": "string.other.link", "settings": { "foreground": "#FFC887FF" } },
    { "scope": "support", "settings": { "foreground": "#FFC887FF" } },
    { "scope": "support.function", "settings": { "foreground": "#7cffcdFF" } },
    { "scope": "support.variable", "settings": { "foreground": "#FF99C0FF" } },
    {
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": { "foreground": "#FF99C0FF" }
    },
    {
      "scope": "support.type.property-name.css",
      "settings": { "foreground": "#BBBBBB" }
    },
    {
      "scope": ["variable.language"],
      "settings": { "foreground": "#FFC887FF", "fontStyle": "italic" }
    },
    { "scope": "variable.parameter", "settings": { "fontStyle": "italic" } },
    {
      "scope": "string.template meta.embedded",
      "settings": { "foreground": "#BBBBBB" }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": { "foreground": "#FF99C0FF", "fontStyle": "bold" }
    },
    {
      "scope": "punctuation.separator",
      "settings": { "foreground": "#BBBBBB" }
    },
    {
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.quasi.element"
      ],
      "settings": { "foreground": "#C6A7FFFF" }
    },
    {
      "scope": "punctuation.section.embedded",
      "settings": { "foreground": "#C6A7FFFF" }
    },
    {
      "scope": "punctuation.definition.list",
      "settings": { "foreground": "#FFC887FF" }
    }
  ]
}
